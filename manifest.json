{
  "manifest_version": 3,
  "name": "CPE",
  "version": "1.0",
  "description": "capacitateparaelempleo.org exam helper",
  "action": {
    "default_icon": "icon.svg",
    "default_title": "CPE",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "options_ui": {
      "page": "options.html"
  },
  "content_scripts": [
    {
      "matches": ["*://capacitateparaelempleo.org/*"],
      "js": ["content.js", 
      "js/getExamData.js",
      "js/setExamFull.js",
      "js/delExamFull.js",
      "js/viewExamFull.js"]
    }
  ]
}

